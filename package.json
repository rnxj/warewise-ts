{
  "name": "saas-kit",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "deploy": "vite build && wrangler deploy",
    "serve": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "cf-typegen": "wrangler types --env-interface Env",
    "better-auth:generate": "npx @better-auth/cli@latest generate --config ./src/lib/auth/index.ts --output ./src/lib/db/auth-schema.ts",
    "drizzle:generate": "drizzle-kit generate --config drizzle.config.ts",
    "drizzle:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "drizzle:migrate:local": "wrangler d1 migrations apply warewise-db --local",
    "drizzle:pull": "drizzle-kit pull --config drizzle.config.ts",
    "precommit": "lint-staged",
    "prepare": "husky",
    "check": "biome check --write ."
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@tanstack/react-query": "^5.90.3",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.133.3",
    "@tanstack/react-router-devtools": "^1.133.3",
    "@tanstack/react-router-ssr-query": "^1.133.3",
    "@tanstack/react-start": "^1.133.3",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "better-auth": "^1.3.27",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.44.6",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.476.0",
    "motion": "^12.23.24",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.65.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.14",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.0",
    "@cloudflare/vite-plugin": "^1.13.13",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.18.10",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/remark-prism": "^1.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "better-sqlite3": "^12.4.1",
    "drizzle-kit": "^0.31.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "typescript": "^5.9.3",
    "ultracite": "5.2.3",
    "vite": "7.1.2",
    "vitest": "^3.2.4",
    "web-vitals": "^4.2.4",
    "wrangler": "^4.43.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,scss,md,mdx}": [
      "biome check ."
    ]
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "better-sqlite3",
      "esbuild",
      "sharp",
      "workerd"
    ]
  },
  "packageManager": "pnpm@10.18.3"
}
